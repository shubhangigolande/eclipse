package basicjava;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class day3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\Amol\\Downloads\\chromedriver\\chromedriver\\chromedriver.exe");
        ChromeDriver driver = new ChromeDriver();
        driver.get("https://codenboxautomationlab.com/contact/");
        
        WebElement ele1 = driver.findElement(By.cssSelector("#nf-field-1"));
        ele1.sendKeys("rakesh");
        
        WebElement ele2 = driver.findElement(By.cssSelector("#nf-field-2"));
        ele2.sendKeys("rakesh@gmail.com");
        
        WebElement ele3= driver.findElement(By.cssSelector("#nf-field-3"));
        ele3.sendKeys("hello!!!");
        
        WebElement ele4 = driver.findElement(By.cssSelector("#nf-field-4"));
        ele4.click();
        
        WebElement ele5 = driver.findElement(By.cssSelector(".nf-response-msg p"));
        String text = ele5.getText()        ;
        System.out.println(text);
        
        if(text.contains("Form submitted successfully.")) {
        	System.out.println("testcase pass");
        }
        else {
        	System.out.println("test case fail");
        }
        
	}

}
